{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/Internals/**",
            "**/Ddap.CodeGen/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net10.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "_site/**",
          "locales/**"
        ]
      },
      {
        "files": [
          "locales/en/*.md",
          "locales/en/toc.yml"
        ],
        "dest": "."
      },
      {
        "files": [
          "locales/pt-br/*.md",
          "locales/pt-br/toc.yml"
        ],
        "dest": "pt-br"
      },
      {
        "files": [
          "locales/es/*.md",
          "locales/es/toc.yml"
        ],
        "dest": "es"
      },
      {
        "files": [
          "locales/fr/*.md",
          "locales/fr/toc.yml"
        ],
        "dest": "fr"
      },
      {
        "files": [
          "locales/de/*.md",
          "locales/de/toc.yml"
        ],
        "dest": "de"
      },
      {
        "files": [
          "locales/ja/*.md",
          "locales/ja/toc.yml"
        ],
        "dest": "ja"
      },
      {
        "files": [
          "locales/zh/*.md",
          "locales/zh/toc.yml"
        ],
        "dest": "zh"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "*.js",
          "*.css",
          "*.html"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "advanced",
        "diagrams"
      ]
    },
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "DDAP - Dynamic Data API Provider",
      "_appName": "DDAP",
      "_appFooter": "DDAP - Dynamic Data API Provider | MIT License",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/schivei/ddap",
        "branch": "main"
      },
      "_languages": {
        "en": {
          "name": "English",
          "path": "/"
        },
        "pt-br": {
          "name": "Português (Brasil)",
          "path": "/pt-br/"
        },
        "es": {
          "name": "Español",
          "path": "/es/"
        },
        "fr": {
          "name": "Français",
          "path": "/fr/"
        },
        "de": {
          "name": "Deutsch",
          "path": "/de/"
        },
        "ja": {
          "name": "日本語",
          "path": "/ja/"
        },
        "zh": {
          "name": "中文",
          "path": "/zh/"
        }
      }
    }
  }
}
