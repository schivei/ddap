{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/Internals/**",
            "**/Ddap.CodeGen/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net10.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "advanced",
        "diagrams"
      ]
    },
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "DDAP - Dynamic Data API Provider",
      "_appName": "DDAP",
      "_appFooter": "DDAP - Dynamic Data API Provider | MIT License",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/schivei/ddap",
        "branch": "main"
      }
    }
  }
}
